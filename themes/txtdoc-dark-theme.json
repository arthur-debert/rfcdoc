{
  "name": "TxtDoc Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1E1E",
    "editor.foreground": "#FFF",
    "editorLineNumber.foreground": "#858585",
    "editor.selectionBackground": "#264F78",
    "editor.selectionHighlightBackground": "#2D2D30",
    "textBlockQuote.border": "#569CD6",
    "textBlockQuote.background": "#666",
    "textCodeBlock.background": "#0D0D0D50"
  },
  "tokenColors": [
    {
      "scope": "markup.heading.txtdoc",
      "settings": {
        "foreground": "#AAA",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.section.txtdoc",
      "settings": {
        "foreground": "#999",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.list.unnumbered.txtdoc",
        "markup.list.numbered.txtdoc",
        "markup.list.lettered.txtdoc",
        "markup.list.roman.txtdoc"
      ],
      "settings": {
        "foreground": "#333"
      }
    },
    {
      "scope": [
        "markup.list.number.txtdoc",
        "markup.list.letter.txtdoc",
        "markup.list.roman.txtdoc"
      ],
      "settings": {
        "foreground": "#666",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.list.item.txtdoc",
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "scope": "markup.raw.block.txtdoc",
      "settings": {
        "foreground": "#999",
        "background": "#1E1E1E"
        
      }
    },
    {
      "scope": [
        "markup.quote.content.txtdoc",
        "markup.quote.nested.content.txtdoc"
      ],
      "settings": {
        "foreground": "#888",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.quote.nested.content.txtdoc",
      "settings": {
        "foreground": "#888",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.italic.txtdoc",
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
}