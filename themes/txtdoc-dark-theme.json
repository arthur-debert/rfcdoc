{
  "name": "RfcDoc Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1E1E",
    "editor.foreground": "#FFF",
    "editorLineNumber.foreground": "#rgb(100, 100, 100)",
    "editor.selectionBackground": "#264F78",
    "editor.selectionHighlightBackground": "#2D2D30",
    "textBlockQuote.border": "#569CD6",
    "textBlockQuote.background": "#666",
    "textCodeBlock.background": "#0D0D0D50"
  },
  "tokenColors": [
    {
      "scope": "markup.heading.rfcdoc",
      "settings": {
        "foreground": "#AAA",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.section.rfcdoc",
      "settings": {
        "foreground": "#999",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.list.unnumbered.rfcdoc",
        "markup.list.numbered.rfcdoc",
        "markup.list.lettered.rfcdoc",
        "markup.list.roman.rfcdoc"
      ],
      "settings": {
        "foreground": "#333"
      }
    },
    {
      "scope": [
        "markup.list.number.rfcdoc",
        "markup.list.letter.rfcdoc",
        "markup.list.roman.rfcdoc"
      ],
      "settings": {
        "foreground": "#666",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.list.item.rfcdoc",
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "scope": "markup.raw.block.rfcdoc",
      "settings": {
        "foreground": "#999",
        "background": "#1E1E1E"
        
      }
    },
    {
      "scope": [
        "markup.quote.content.rfcdoc",
        "markup.quote.nested.content.rfcdoc"
      ],
      "settings": {
        "foreground": "#888",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.quote.nested.content.rfcdoc",
      "settings": {
        "foreground": "#888",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.italic.rfcdoc",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.bold.rfcdoc",
      "settings": {
        "fontStyle": "bold"
      }
    }
  ]
}