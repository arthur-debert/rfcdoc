{
  "name": "RfcDoc Light",
  "type": "light",
  "colors": {
    "editor.background": "#FFFFFF",
    "editor.foreground": "#000000",
    "editorLineNumber.foreground": "#999999",
    "editor.selectionBackground": "#ADD6FF",
    "editor.selectionHighlightBackground": "#E5EBF1",
    "textBlockQuote.border": "#FF0000",
    "textBlockQuote.background": "#0078D420",
    "textCodeBlock.background": "#00000010"
  },
  "tokenColors": [
    {
      "scope": "markup.heading.rfcdoc",
      "settings": {
        "foreground": "#999",
        "background": "#AAA",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.section.rfcdoc",
      "settings": {
        "foreground": "#666",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.list.unnumbered.rfcdoc",
        "markup.list.numbered.rfcdoc",
        "markup.list.lettered.rfcdoc",
        "markup.list.roman.rfcdoc"
      ],
      "settings": {
        "foreground": "#A31515"
      }
    },
    {
      "scope": [
        "markup.list.number.rfcdoc",
        "markup.list.letter.rfcdoc",
        "markup.list.roman.rfcdoc"
      ],
      "settings": {
        "foreground": "#098658",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.list.item.rfcdoc",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": "markup.raw.block.rfcdoc",
      "settings": {
        "foreground": "#795E26",
        "background": "#F6F6F6"
      }
    },
    {
      "scope": [
        "markup.quote.content.rfcdoc",
        "markup.quote.nested.content.rfcdoc"
      ],
      "settings": {
        "foreground": "#008000",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.quote.nested.content.rfcdoc",
      "settings": {
        "foreground": "#006600",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.italic.rfcdoc",
      "settings": {
        "foreground": "#AF00DB",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.bold.rfcdoc",
      "settings": {
        "foreground": "#0000FF",
        "fontStyle": "bold"
      }
    }
  ]
}
