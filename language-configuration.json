{
  "comments": {
    // We don't have specific comment syntax in this format
  },
  "brackets": [
    // No specific bracket pairs
  ],
  "autoClosingPairs": [["_", "_"]],
  "surroundingPairs": [
    ["_", "_"],
    [">", ""]
  ],
  "folding": {
    "markers": {
      "start": "^([A-Za-z0-9].*$\\n^-+$|^[A-Z][^\\n]*$|^\\s{4})",
      "end": "^(?!\\s{4})$"
    }
  },
  "wordPattern": ":[a-zA-Z0-9_]+|\\w+",
  "indentationRules": {
    "increaseIndentPattern": "^(\\s*-\\s+|\\s*\\d+\\.\\s+|\\s*[a-z]\\.\\s+|\\s*[ivx]+\\.\\s+)",
    "decreaseIndentPattern": "^\\s*$"
  }
}
